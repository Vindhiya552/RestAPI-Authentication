
<h3>Edit User</h1>


 {{#if msg}}
<div class="alert">{{msg}}</div>
{{/if}} 
{{#each rows}}
<form method="post" action="/edituser/{{this.Id}}" autocomplete="false">
<div class="form-group">
<label for="name">
Name
</label>
<input type="text" name="name" value="{{this.name}}">
</div>

<div class="form-group">
<label for="age">
Age
</label>
<input type="text" name="age" value="{{this.age}}"> 
</div>

<div class="form-group">
<label for="city">
City
</label>
<input type="text" name="city" value="{{this.city}}">
</div>

<div class="form-group">
<button type="submit">Save Changes</button>
<button ><a href="/">ViewAll</a></button>
</div>
</form>
{{/each}}
